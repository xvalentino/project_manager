<h1>
  <%= @board.title %>
</h1>
<br>

<div class="row">
  <div class="col s3">

    <% @tickets.each do |ticket| %>
      <%# <% if ticket.backlog? %1> %>
        <p> <%= ticket.title %></p>
        <p> <%= ticket.status %></p>
        <%# <% end %1> %>
    <% end %>

  </div>
</div>

<div class="row">
  <%= form_for @ticket do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.hidden_field :board_id, :value => @board.id %>
    <%= f.submit %>
  <% end %>
</div>


<%= link_to "Home", root_path %>

